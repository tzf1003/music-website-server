        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaosong.music.server.mapper.SheetMapper">

    <resultMap id="BaseResultMap" type="com.xiaosong.music.server.domain.Sheet">
